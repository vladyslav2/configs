// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "cmd+g",
    "command": "workbench.action.keepEditor"
  },
  {
    "key": "cmd+k enter",
    "command": "-workbench.action.keepEditor"
  },
  {
    "key": "cmd+u cmd+u",
    "command": "cursorUndo",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+u",
    "command": "-cursorUndo",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+u",
    "command": "editor.action.sortLinesAscending"
  }
]